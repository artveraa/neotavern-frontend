{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f5f5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["node_modules/@expo-google-fonts/Lexend_100Thin.ttf", "node_modules/@expo-google-fonts/Lexend_200ExtraLight.ttf", "node_modules/@expo-google-fonts/Lexend_300Light.ttf","node_modules/@expo-google-fonts/Lexend_400Regular.ttf","node_modules/@expo-google-fonts/Lexend_500Medium.ttf","node_modules/@expo-google-fonts/Lexend_600SemiBold.ttf","node_modules/@expo-google-fonts/Lexend_700Bold.ttf","node_modules/@expo-google-fonts/Lexend_800ExtraBold.ttf","node_modules/@expo-google-fonts/Lexend_900Black.ttf"]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
